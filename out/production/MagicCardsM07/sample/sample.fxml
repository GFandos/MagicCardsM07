                <Button mnemonicParsing="false" onAction="#getNewCards" text="Get cards" />
              </items>
            </ToolBar>
            <ListView fx:id="cardsListView" layoutX="-1.0" layoutY="64.0" prefHeight="475.0" prefWidth="309.0" />
